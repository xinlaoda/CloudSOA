<?xml version="1.0" encoding="utf-8" ?>
<ServiceRegistration xmlns="urn:cloudsoa:service-config">
  <ServiceName>HeavyComputeService</ServiceName>
  <Version>1.0.0</Version>
  <Runtime>wcf-netfx</Runtime>
  <AssemblyName>HeavyComputeService.dll</AssemblyName>
  <ServiceContractType>HeavyComputeService.IHeavyCompute</ServiceContractType>
  <Resources>
    <MinInstances>1</MinInstances>
    <MaxInstances>10</MaxInstances>
    <CpuPerInstance>500m</CpuPerInstance>
    <MemoryPerInstance>512Mi</MemoryPerInstance>
  </Resources>
</ServiceRegistration>
