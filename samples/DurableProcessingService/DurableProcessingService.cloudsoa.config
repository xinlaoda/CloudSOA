<?xml version="1.0" encoding="utf-8"?>
<ServiceRegistration xmlns="urn:cloudsoa:service-config">
  <ServiceName>DurableProcessingService</ServiceName>
  <Version>1.0.0</Version>
  <Runtime>wcf-netfx</Runtime>
  <AssemblyName>DurableProcessingService.dll</AssemblyName>
  <ServiceContractType>DurableProcessingService.IDurableProcessor</ServiceContractType>
  <Resources>
    <MinInstances>1</MinInstances>
    <MaxInstances>5</MaxInstances>
    <CpuPerInstance>500m</CpuPerInstance>
    <MemoryPerInstance>512Mi</MemoryPerInstance>
  </Resources>
</ServiceRegistration>
