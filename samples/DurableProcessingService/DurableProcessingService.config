<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <microsoft.Hpc.Session.ServiceRegistration>
    <service assembly="%CCP_HOME%\DurableProcessingService.dll"
             contract="DurableProcessingService.IDurableProcessor"
             type="DurableProcessingService.DurableProcessorImpl"
             includeExceptionDetailInFaults="true"
             maxConcurrentCalls="8">
      <environmentVariables>
        <add name="MAX_ITERATIONS" value="10000" />
      </environmentVariables>
    </service>
  </microsoft.Hpc.Session.ServiceRegistration>
</configuration>
